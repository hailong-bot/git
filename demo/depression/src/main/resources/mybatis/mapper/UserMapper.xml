<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chl.depression.mapper.UserMapper">
    <insert id="InsertUser"  parameterType="com.chl.depression.bean.User">
        INSERT INTO user(id,phone,password,create_time,update_time,type)
        values (#{id},#{phone},#{password},#{create_time},#{update_time},#{type})
    </insert>
</mapper>